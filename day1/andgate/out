#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x62d21320c300 .scope module, "and_tb" "and_tb" 2 1;
 .timescale 0 0;
v0x62d21321e910_0 .var "i0", 0 0;
v0x62d21321e9e0_0 .var "i1", 0 0;
v0x62d21321eab0_0 .net "out", 0 0, L_0x62d21321ebb0;  1 drivers
S_0x62d21320c490 .scope module, "uut" "and_gate" 2 4, 3 1 0, S_0x62d21320c300;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "i0";
    .port_info 1 /INPUT 1 "i1";
    .port_info 2 /OUTPUT 1 "out";
L_0x62d21321ebb0 .functor AND 1, v0x62d21321e910_0, v0x62d21321e9e0_0, C4<1>, C4<1>;
v0x62d21320c6c0_0 .net "i0", 0 0, v0x62d21321e910_0;  1 drivers
v0x62d21321e730_0 .net "i1", 0 0, v0x62d21321e9e0_0;  1 drivers
v0x62d21321e7f0_0 .net "out", 0 0, L_0x62d21321ebb0;  alias, 1 drivers
    .scope S_0x62d21320c300;
T_0 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62d21321e910_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62d21321e9e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62d21321e910_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62d21321e9e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62d21321e910_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62d21321e9e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62d21321e910_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62d21321e9e0_0, 0, 1;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x62d21320c300;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "and_tb.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %vpi_call 2 17 "$monitor", "Time=%0t | I0 =%b | I1 =%b | out=%b", $time, v0x62d21321e910_0, v0x62d21321e9e0_0, v0x62d21321eab0_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "andgate_tb.v";
