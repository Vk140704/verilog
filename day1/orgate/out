    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6234688b7130_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6234688b7060_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6234688b7130_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6234688b7060_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6234688b7130_0, 0, 1;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x62346886b490;
T_1 ;
