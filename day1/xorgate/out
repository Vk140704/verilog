#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x654543c6a490 .scope module, "xor_tb" "xor_tb" 2 1;
 .timescale 0 0;
v0x654543cb5b80_0 .var "a", 0 0;
v0x654543cb5c50_0 .var "b", 0 0;
v0x654543cb5d20_0 .net "out", 0 0, L_0x654543cb5e20;  1 drivers
S_0x654543c6a620 .scope module, "uut" "xorgate" 2 4, 3 1 0, S_0x654543c6a490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "out";
L_0x654543cb5e20 .functor XOR 1, v0x654543cb5b80_0, v0x654543cb5c50_0, C4<0>, C4<0>;
v0x654543c6a850_0 .net "a", 0 0, v0x654543cb5b80_0;  1 drivers
v0x654543cb59a0_0 .net "b", 0 0, v0x654543cb5c50_0;  1 drivers
v0x654543cb5a60_0 .net "out", 0 0, L_0x654543cb5e20;  alias, 1 drivers
    .scope S_0x654543c6a490;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x654543cb5b80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x654543cb5c50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x654543cb5b80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x654543cb5c50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x654543cb5b80_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x654543cb5c50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x654543cb5b80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x654543cb5c50_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x654543c6a490;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "xor_tb.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %vpi_call 2 15 "$monitor", "%0t | A =%b | B=%b | OUT=%b", $time, v0x654543cb5b80_0, v0x654543cb5c50_0, v0x654543cb5d20_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
