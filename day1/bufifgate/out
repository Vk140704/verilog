#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b284517d8c0 .scope module, "bufif_tb" "bufif_tb" 2 1;
 .timescale 0 0;
v0x5b28451a3690_0 .var "a", 0 0;
v0x5b28451a3760_0 .var "control", 0 0;
v0x5b28451a3830_0 .net "y", 0 0, v0x5b28451a3570_0;  1 drivers
S_0x5b2845157490 .scope module, "uut" "bufifgate" 2 4, 3 1 0, S_0x5b284517d8c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "control";
    .port_info 2 /OUTPUT 1 "y";
v0x5b28451576e0_0 .net "a", 0 0, v0x5b28451a3690_0;  1 drivers
v0x5b28451a34b0_0 .net "control", 0 0, v0x5b28451a3760_0;  1 drivers
v0x5b28451a3570_0 .var "y", 0 0;
E_0x5b2845190da0 .event anyedge, v0x5b28451a34b0_0, v0x5b28451576e0_0;
    .scope S_0x5b2845157490;
T_0 ;
    %wait E_0x5b2845190da0;
    %load/vec4 v0x5b28451a34b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5b28451576e0_0;
    %store/vec4 v0x5b28451a3570_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 1, 1;
    %store/vec4 v0x5b28451a3570_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5b284517d8c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b28451a3690_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b28451a3760_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b28451a3690_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b28451a3760_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b28451a3690_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b28451a3760_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b28451a3690_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b28451a3760_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5b284517d8c0;
T_2 ;
    %vpi_call 2 13 "$dumpfile", "bufif_tb.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %vpi_call 2 15 "$monitor", "%0t | A=%b | CONTROL=%b | Y=%b ", $time, v0x5b28451a3690_0, v0x5b28451a3760_0, v0x5b28451a3830_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "bufifgate_tb.v";
    "bufifgate.v";
