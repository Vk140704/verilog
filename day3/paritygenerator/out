#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x63fab463a4c0 .scope module, "parity_tb" "parity_tb" 2 1;
 .timescale 0 0;
v0x63fab4685a70_0 .var "a", 3 0;
v0x63fab4685b30_0 .net "y", 0 0, v0x63fab4685950_0;  1 drivers
S_0x63fab463a650 .scope module, "uut" "paritygen" 2 4, 3 5 0, S_0x63fab463a4c0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /OUTPUT 1 "y";
v0x63fab463a850_0 .net "a", 3 0, v0x63fab4685a70_0;  1 drivers
v0x63fab4685950_0 .var "y", 0 0;
E_0x63fab4673d80 .event anyedge, v0x63fab463a850_0;
    .scope S_0x63fab463a650;
T_0 ;
    %wait E_0x63fab4673d80;
    %load/vec4 v0x63fab463a850_0;
    %xor/r;
    %store/vec4 v0x63fab4685950_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x63fab463a4c0;
T_1 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x63fab4685a70_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x63fab4685a70_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x63fab4685a70_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x63fab4685a70_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x63fab4685a70_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x63fab4685a70_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x63fab463a4c0;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "parity_tb.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %vpi_call 2 17 "$monitor", "%0t | A=%b | Y=%b ", $time, v0x63fab4685a70_0, v0x63fab4685b30_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "parity_tb.v";
    "paritygen.v";
